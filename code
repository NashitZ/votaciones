class Cantidate:
    ##atributos##
    name=''
    political_party=''
    vote=0
    id=0

    ##fin atributos##
    ##metodos##
    def __init__(self,id,name,political_party,votes):
        self.id=id ##TODO que setee autoincrementble
        self.name=name
        self.vote=vote
        self.political_party=political_party

    def suma_votes(self):
        self.vote=vote+1

    ##fin metodos##

name=''
political_party=''
vote=0
id=0   

list = []
list2 = []
tamano=2


for x in range(tamano):
    print("ingrese los datos de la persona",x+1)
    name=input("nombre: ")
    political_party=input("partido politico: ")
    
    list.append(name)
    list2.append(political_party)

    
for x in range(tamano):
    print("datos de la persona",x+1)
    print("nombre: ",list[x])
    print("partido: ",list2[x])

